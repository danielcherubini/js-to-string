{
  "name": "js-to-string",
  "version": "0.4.5",
  "description": "Turns anything to string",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib",
    "test": "tests"
  },
  "dependencies": {
    "serialize-javascript": "^1.5.0"
  },
  "devDependencies": {
    "@types/node": "^12.12.1",
    "ava": "^0.25.0",
    "nyc": "^11.8.0",
    "require-from-string": "^2.0.2",
    "tslint": "^5.10.0",
    "typescript": "^2.8.3"
  },
  "scripts": {
    "test": "nyc ava",
    "coverage": "nyc report --reporter=lcov > coverage.lcov && codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danmademe/stringit.git"
  },
  "keywords": [
    "javascript",
    "node",
    "string"
  ],
  "author": "Daniel Cherubini",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/danmademe/stringit/issues"
  },
  "ava": {
    "files": [
      "tests/**/*.js",
      "!tests/example/**/*.js"
    ],
    "source": [
      "lib/**/*.js"
    ],
    "concurrency": 5,
    "failFast": false,
    "powerAssert": false,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "include": [
      "lib/**/*.js"
    ],
    "exclude": [
      "lib/assets/**/*"
    ],
    "reporter": [
      "text-summary"
    ]
  },
  "homepage": "https://github.com/danmademe/stringit#readme"
}
